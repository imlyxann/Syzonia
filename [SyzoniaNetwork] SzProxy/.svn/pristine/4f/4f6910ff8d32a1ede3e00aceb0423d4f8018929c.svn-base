package fr.syzonia.syzobungee.server;

public class MiniServer {

	String ip, name, type;
	int port;
	
	
	public MiniServer(String name, int port, String ip, String Type) {
		this.name = name;
		this.ip = ip;
		this.port = port;
		this.type = Type;
	}
	
	public int getPort() {
		return port;
	}
	
	public String getIp() {
		return ip;
	}
	
	public String getName() {
		return name;
	}
	
	public String getType() {
		return type;
	}
	
}
