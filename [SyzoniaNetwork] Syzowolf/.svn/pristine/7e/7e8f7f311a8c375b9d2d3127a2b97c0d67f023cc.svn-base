package fr.syzonia.syzowolf.utils;

import org.bukkit.Location;

public class RawUtils {

	public String LocationToString(Location loc) {
		return "X: " + Math.abs(loc.getX()) + " Y: " + Math.abs(loc.getY()) + " Z: " + Math.abs(loc.getZ());
	}
	
	public String LocationToStringY(Location loc) {
		return "Y: " + Math.abs(loc.getY());
	}
	
}
